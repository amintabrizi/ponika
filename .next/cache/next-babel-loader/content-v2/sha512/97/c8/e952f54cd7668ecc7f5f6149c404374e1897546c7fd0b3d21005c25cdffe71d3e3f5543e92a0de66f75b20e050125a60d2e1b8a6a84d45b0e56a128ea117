} = form\r\n\r\n    return name && userName && email && password && privacyPolicy\r\n  }\r\n\r\n  let handleInput = (e, inputName) => {\r\n    setForm({ ...form, [inputName]: e.target.value })\r\n    if (!!errors[inputName]) setErrors({\r\n      ...errors,\r\n      [inputName]: null\r\n    })\r\n  }\r\n\r\n  let handleCheckBox = () => {\r\n    setForm({ ...form, privacyPolicy: !form.privacyPolicy })\r\n  }\r\n\r\n  const findFormErrors = () => {\r\n    const { name, userName, email, password, privacyPolicy } = form\r\n    const newErrors = {}\r\n    // name errors\r\n    if (!name || name === '') newErrors.name = 'Can not be blank!'\r\n    else if (name.length < 2) newErrors.name = 'Name is too short!'\r\n    else if (name.length > 30) newErrors.name = 'Name is too long!'\r\n\r\n    if (!userName || userName === '') newErrors.userName = 'Can not be blank!'\r\n    else if (userName.length < 2) newErrors.userName = 'User name is too short!'\r\n    else if (userName.length > 30) newErrors.userName = 'User name is too long!'\r\n    else if (!userName.match(/^[a-z0-9_]{5,}[a-z]+[0-9]*$/)) {\r\n      var msg = [];\r\n      if (!/[a-z0-9_]/.test(userName)) {                 // Are there any letters?\r\n        msg.push('dont use symbole');\r\n      }\r\n      if (!/[a-z0-9_]{5,}[a-z]+[0-9]/.test(userName)) {                       // Are there any digits?\r\n        msg.push(\"you can only use letters and digits and _\");\r\n      }\r\n      if (userName.length < 6) {                        // Is the length 6+?\r\n        msg.push(\"Word is less than 6 chars long. \");\r\n      }\r\n      console.log(msg);\r\n      newErrors.userName = msg[0]\r\n    }\r\n\r\n    if (!email || email === '') newErrors.email = 'Can not be blank!'\r\n    else if (email.length < 2) newErrors.email = 'User name is too short!'\r\n    else if (!email.match(/[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,15}/g)) newErrors.email = 'Email is not valid'\r\n\r\n    if (!password || password === '') newErrors.password = 'Can not be blank!'\r\n    else if (password.length < 2) newErrors.password = 'Password is too short!'\r\n    else if (password.length > 30) newErrors.password = 'Password is too long!'\r\n\r\n\r\n\r\n    return newErrors\r\n  }\r\n\r\n  const handleSubmit = e => {\r\n    e.preventDefault()\r\n    // get our new errors\r\n    const newErrors = findFormErrors()\r\n    // Conditional logic:\r\n    if (Object.keys(newErrors).length > 0) {\r\n      // We got errors!\r\n      setErrors(newErrors)\r\n    } else {\r\n      // No errors! Put any logic here for the form submission!\r\n      alert('Thank you for your feedback!')\r\n    }\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <Modal.Header className={`${style.modalHeader} position-relative`}>\r\n\r\n        <button\r\n          onClick={() => props.modalAction()}\r\n          type=\"button\"\r\n          className={`${style.close} position-absolute bg-white border border-white p-0`}\r\n        >\r\n          <Image src=\"/svg/close.svg\" height={30} width={30} />\r\n        </button>\r\n\r\n        <Modal.Title\r\n          className={`${style.modalTitle} font-weight-normal`}\r\n          id=\"contained-modal-title-vcenter\"\r\n        >Sign up</Modal.Title>\r\n\r\n      </Modal.Header>\r\n\r\n      <Modal.Body className=\"mx-4 px-0\">\r\n        <div className=\"social-login d-flex my-4 pb-2\">\r\n          <Link href=\"http://google.com\" passHref>\r\n            <Button className=\"border mr-3 flex-grow-1\" variant=\"light\">\r\n              <img src=\"/svg/google.svg\" className=\"mr-1\" height={24} width={24} />Sign up with Google\r\n            </Button>\r\n          </Link>\r\n          <Link href=\"http://facebook.com\" passHref>\r\n            <Button variant=\"info\"><img src=\"/svg/facebook.svg\" height={24} width={24} /></Button>\r\n          </Link>\r\n        </div>\r\n\r\n        <hr className={`${style.divider} divider my-4`}></hr>\r\n\r\n        <div className=\"social-login d-flex mt-4\">\r\n   