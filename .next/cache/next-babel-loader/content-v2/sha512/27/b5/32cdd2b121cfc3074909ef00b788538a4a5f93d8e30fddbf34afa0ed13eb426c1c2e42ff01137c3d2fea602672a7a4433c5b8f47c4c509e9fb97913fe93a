  }\r\n\r\n  const [form, setForm] = useState(initialState);\r\n  const [errors, setErrors] = useState({})\r\n\r\n\r\n  let isFormValid = () => {\r\n    const { fullName, userName, email, password, privacyPolicy } = form\r\n\r\n    return fullName && userName && email && password && privacyPolicy\r\n  }\r\n\r\n  let handleInput = (e, inputName) => {\r\n    setForm({ ...form, [inputName]: e.target.value })\r\n    if (!!errors[inputName]) setErrors({\r\n      ...errors,\r\n      [inputName]: null\r\n    })\r\n  }\r\n\r\n  let handleCheckBox = () => {\r\n    setForm({ ...form, privacyPolicy: !form.privacyPolicy })\r\n  }\r\n\r\n  const findFormErrors = () => {\r\n    const { fullName, userName, email, password, privacyPolicy } = form\r\n    const newErrors = {}\r\n    // fullName errors\r\n    if (!fullName || fullName === '') newErrors.fullName = 'Can not be blank!'\r\n    else if (fullName.length < 2) newErrors.fullName = 'Full name is too short!'\r\n    else if (fullName.length > 30) newErrors.fullName = 'Full name is too long!'\r\n    else if (!fullName.match(/^[a-z]{3,}([',. -][a-z]{3,})*$/)) {\r\n      var msg = [];\r\n      if (/[a-z]/.test(userName)) {\r\n        msg.push('you can only use lowercase characters');\r\n      }\r\n      console.log(msg);\r\n      newErrors.userName = msg[0]\r\n    }\r\n\r\n    if (!userName || userName === '') newErrors.userName = 'Can not be blank!'\r\n    else if (!userName.match(/^(?![_ - 0-9])(?:(?![_ -]{2})[\\w -]){5,16}(?<![_ -])$/)) {\r\n      var msg = [];\r\n      if (/(?![_ - 0-9])/.test(userName)) {                 // Are there any letters?\r\n        msg.push('you can not use _ and . at the begining');\r\n      }\r\n      else if (/(?![_ -]{2})/.test(userName)) {                 // Are there any letters?\r\n        msg.push('you can not use _ and - that immediately follow each other.');\r\n      }\r\n      console.log(msg);\r\n      newErrors.userName = msg[0]\r\n    }\r\n\r\n    if (!email || email === '') newErrors.email = 'Can not be blank!'\r\n    else if (email.length < 2) newErrors.email = 'User name is too short!'\r\n    else if (!email.match(/[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,15}/g)) newErrors.email = 'Email is not valid'\r\n\r\n    if (!password || password === '') newErrors.password = 'Can not be blank!'\r\n    else if (password.length < 2) newErrors.password = 'Password is too short!'\r\n    else if (password.length > 30) newErrors.password = 'Password is too long!'\r\n\r\n\r\n\r\n    return newErrors\r\n  }\r\n\r\n  const handleSubmit = e => {\r\n    e.preventDefault()\r\n    // get our new errors\r\n    const newErrors = findFormErrors()\r\n    // Conditional logic:\r\n    if (Object.keys(newErrors).length > 0) {\r\n      // We got errors!\r\n      setErrors(newErrors)\r\n    } else {\r\n      // No errors! Put any logic here for the form submission!\r\n      alert('Thank you for your feedback!')\r\n    }\r\n  }\r\n\r\n  return (\r\n    <>\r\n      <Modal.Header className={`${style.modalHeader} position-relative`}>\r\n\r\n        <button\r\n          onClick={() => props.modalAction()}\r\n          type=\"button\"\r\n          className={`${style.close} position-absolute bg-white border border-white p-0`}\r\n        >\r\n          <Image src=\"/svg/close.svg\" height={30} width={30} />\r\n        </button>\r\n\r\n        <Modal.Title\r\n          className={`${style.modalTitle} font-weight-normal`}\r\n          id=\"contained-modal-title-vcenter\"\r\n        >Sign up</Modal.Title>\r\n\r\n      </Modal.Header>\r\n\r\n      <Modal.Body className=\"mx-4 px-0\">\r\n        <div className=\"social-login d-flex my-4 pb-2\">\r\n          <Link href=\"http://google.com\" passHref>\r\n            <Button className=\"border mr-3 flex-grow-1\" variant=\"light\">\r\n              <img src=\"/svg/google.svg\" className=\"mr-1\" height={24} width={24} />Sign up with Google\r\n            </Button>\r\n          </Link>\r\n          <Link href=\"http://facebook.com\" passHref>\r\n            <Button variant=\"info\"><img src=\"/svg/facebook.svg\" height={24} width={24} /></Button>\r\n          </Link>\r\n        </div>\r\n\r\n        <hr className={`${style.divider} divider my-4`}></hr>\r\n\r\n        <div className=\"social-login d-flex mt-4\">\r\n          <Form className=\"w-100\" onSubmit={(e) => handleSubmit(e)} noValidate>\r\n            <Row className=\"mb-4\">\r\n              <Col>\r\n                <Form.Control onChange={(e) => handleInput(e, 'fullName')} placeholder=\"Full Name\"\r\n                  isInvalid={!!errors.fullName} />\r\n                <Form.Control.Feedback type='invalid' tooltip style={{ left: '15px' }}>\r\n                  {errors.fullName}\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n              <Col>\r\n                <Form.Control onChange={(e) => handleInput(e, 'userName')} placeholder=\"User name\"\r\n                  isInvalid={!!errors.userName} />\r\n                <Form.Control.Feedback type='invalid' tooltip style={{ left: '15px' }}>\r\n                  {errors.userName}\r\n                </Form.Control.Feedback>\r\n              </Col>\r\n            </Row>\r\n            <Form.Group className=\"mb-4 position-relative\" controlId=\"formGroupEmail\">\r\n              <Form.Control onChange={(e) => handleInput(e, 'email')} type=\"email\" placeholder=\"Enter email\"\r\n                isInvalid={!!errors.email} />\r\n              <Form.Control.Feedback type='invalid' tooltip>\r\n                {errors.email}\r\n              </Form.Control.Feedback>\r\n            </Form.Group>\r\n            <Form.Group controlId=\"formGroupPassword\" className=\"mb-4 position-relative\">\r\n              <Form.Control onChange={(e) => handleInput(e, 'password')} type=\"password\" placeholder=\"Password\"\r\n                isInvalid={!!errors.password} />\r\n              <Form.Control.Feedback type='invalid' tooltip>\r\n                {errors.password}\r\n              </Form.Control.Feedback>\r\n            </Form.Group>\r\n            <Col className=\"p-0\">\r\n              <div className=\"custom-control custom-checkbox mr-sm-2\">\r\n                <input onChange={() => handleCheckBox('privacyPolicy')} type=\"checkbox\" className=\"custom-control-input\" id=\"customControlAutosizing\" />\r\n                <label className=\"custom-control-label\" htmlFor=\"customControlAutosizing\">Creating an account means youâ€™re okay with our Terms of Service, and Privacy Policy.</label>\r\n              </div>\r\n            </Col>\r\n            <Col className=\"my-4\">\r\n              <Button type=\"submit\" disabled={!isFormValid()} size=\"md\" block variant=\"danger\" >Create Account</Button>\r\n            </Col>\r\n          </Form>\r\n        </div>\r\n\r\n      </Modal.Body>\r\n\r\n      <Modal.Footer className=\"d-flex justify-content-end\">\r\n        <div onClick={() => props.signInAction()} className=\"py-2\">Already a member? <span className=\"text-primary cursor-pointer\">Sign In</span></div>\r\n      </Modal.Footer>\r\n    </>\r\n  )\r\n}\r\n"]},"metadata":{},"sourceType":"module"}